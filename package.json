{
  "name": "agesture",
  "version": "0.0.1",
  "description": "A browser extension for mouse gestures",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "start": "wxt",
    "build": "wxt build",
    "zip": "wxt zip",
    "postinstall": "wxt prepare",
    "typecheck": "tsc --noEmit",
    "format": "biome check --write --formatter-enabled=true --organize-imports-enabled=true --unsafe",
    "check": "biome check .",
    "test": "vitest run",
    "all": "npm run format && npm run check && npm run typecheck && npm run test",
    "for_agent": "bash ./script/run.sh format && bash ./script/run.sh check && bash ./script/run.sh typecheck && bash ./script/run.sh test",
    "deploy_for_local": "npm run build && mkdir -p $AGESTURE_LOCAL && cp -rf ./.output/chrome-mv3/* $AGESTURE_LOCAL"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@tailwindcss/vite": "^4.1.10",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@wxt-dev/module-react": "^1.1.3",
    "jsdom": "^26.1.0",
    "lefthook": "^1.11.13",
    "tailwindcss": "^4.1.5",
    "typescript": "^5.8.3",
    "vitest": "^3.2.3",
    "wxt": "0.20.7"
  },
  "license": "MIT",
  "dependencies": {
    "clsx": "^2.1.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwind-merge": "^3.3.1",
    "valibot": "^1.1.0"
  }
}
