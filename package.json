{
  "name": "agesture",
  "version": "0.0.1",
  "description": "A browser extension for mouse gestures",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "start": "wxt",
    "build": "wxt build",
    "zip": "wxt zip",
    "postinstall": "wxt prepare",
    "typecheck": "tsc --noEmit",
    "format": "biome check --write --formatter-enabled=true --assist-enabled=true --unsafe",
    "check": "biome check .",
    "test": "vitest run",
    "check_all": "npm run format && npm run check && npm run typecheck && npm run test",
    "deploy_for_local": "npm run build && mkdir -p $AGESTURE_LOCAL && cp -rf ./.output/chrome-mv3/* $AGESTURE_LOCAL"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@tailwindcss/vite": "^4.1.18",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@wxt-dev/module-react": "^1.1.5",
    "lefthook": "^2.1.1",
    "tailwindcss": "^4.1.5",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18",
    "wxt": "0.20.14"
  },
  "license": "MIT",
  "dependencies": {
    "clsx": "^2.1.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwind-merge": "^3.4.0",
    "valibot": "^1.2.0"
  }
}
