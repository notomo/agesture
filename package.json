{
  "name": "agesture",
  "version": "0.0.1",
  "description": "A browser extension for mouse gestures",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "build": "wxt build",
    "zip": "wxt zip",
    "postinstall": "wxt prepare",
    "typecheck": "tsc --noEmit",
    "format": "biome check --write --formatter-enabled=true --organize-imports-enabled=true --unsafe",
    "check": "biome check .",
    "test": "vitest run"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@tailwindcss/vite": "^4.1.5",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "@wxt-dev/module-react": "^1.1.3",
    "jsdom": "^26.1.0",
    "lefthook": "^1.11.12",
    "tailwindcss": "^4.1.5",
    "typescript": "^5.8.3",
    "vitest": "^3.1.2",
    "wxt": "0.20.6"
  },
  "license": "MIT",
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "valibot": "^1.0.0"
  }
}
